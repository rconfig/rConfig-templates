main:
  name: "Cisco IOS SMB - SSH - No Enable"           # Template name for Cisco SMB over SSH
  desc: "Cisco IOS SSH based connection without enable mode"

connect:
  timeout: 30                                       # Connection timeout (in seconds)
  protocol: ssh                                     # Connection protocol: SSH
  port: 22                                          # Default SSH port

auth:
  username: "Username:"                             # SSH username prompt
  password: "Password:"                             # SSH password prompt
  enable: off                                       # Enable mode not required
  enableCmd: "enable"                               # Enable command (unused)
  enablePassPrmpt: "Password:"                      # Prompt for enable password (unused)
  hpAnyKeyStatus: off                               # HP press-any-key handling
  hpAnyKeyPrmpt: "Press any key to continue"        # Prompt string if applicable

config:
  linebreak: "n"                                    # Linebreak type
  paging: on                                        # Disable CLI paging
  pagingCmd: "terminal length 0"                    # Command to disable paging
  resetPagingCmd: "terminal length 40"              # Command to restore paging
  pagerPrompt: ""                                   # Deprecated
  pagerPromptCmd: " "                               # Deprecated
  saveConfig: "wr mem"                              # Save configuration command
  exitCmd: "quit"                                   # Exit command
